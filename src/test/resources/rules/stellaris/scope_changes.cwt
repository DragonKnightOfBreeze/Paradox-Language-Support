	##cardinality = 0..1
	limit = { alias_name[trigger] = alias_match_left[trigger] }
	alias_name[effect] = alias_match_left[effect]

	##cardinality = 0..1
	weights = {
		base = float
		## cardinality = 0..inf
		alias_name[modifier_rule_add_mult] = alias_match_left[modifier_rule_add_mult]

	}
}

