# Test CWT config for multi data_source links

links = {
    test_value_link = {
        type = value
        from_argument = yes
        prefix = test_value
        data_source = value[test_flag]
        data_source = <country>
        input_scopes = { country }
        output_scope = country
    }

    test_scope_link = {
        type = scope
        from_argument = yes
        prefix = test_scope
        data_source = scope_field
        data_source = <planet>
        input_scopes = { planet }
        output_scope = planet
    }
}
