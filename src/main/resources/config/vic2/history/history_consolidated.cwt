
types = {
    type[diplomacy_history] = {
        path = "game/history/diplomacy"
        name_from_file = yes
        type_per_file = yes
    }
	type[country_history] = {
		path = "game/history/countries"
		name_from_file = yes
		type_per_file = yes
	}
	## push_scope = province
	type[pops_history] = {
		path = "game/history/pops/<bookmarks>"
		name_from_file = yes
		type_per_file = yes
	}
	## push_scope = province
	type[province_history] = {
		path = "game/history/provinces/any"
		name_from_file = yes
		type_per_file = yes

		subtype[owned] = {
			owner = scalar
		}
	}

	type[units_history] = {
		path = "game/history/units" #TODO what about <bookmark> sub dirs?
		name_from_file = yes
		type_per_file = yes
		name_field = "name"
	}

	type[war_history] = {
		path = "game/history/wars"
		name_from_file = yes
		type_per_file = yes
		name_field = "name"
	}
}

diplomacy_history = {
    ## cardinality = 0..inf
    enum[other_diplo_history] = {
        first = enum[country_tags]
        second = enum[country_tags]
        start_date = date_field
        end_date = date_field
    }
}

enums = {
	enum[other_diplo_history] = {
		guarantee
		union
		alliance
	}
}

