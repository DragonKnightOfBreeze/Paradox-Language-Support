	## cardinality = 0..1
	infantry = float
	## cardinality = 0..1
	cavalry = float
	## cardinality = 0..1
	artillery = float
	## cardinality = 0..1
	heavy_ship = float
	## cardinality = 0..1
	light_ship = float
	## cardinality = 0..1
	galley = float
	## cardinality = 0..1
	transport = float
	## cardinality = 0..1
	<unit_type> = float #todo
}

