<idea-plugin>
  <extensions defaultExtensionNs="com.intellij">
    <fileType language="PARADOX_CSV" name="Paradox Csv" fieldName="INSTANCE" implementationClass="icu.windea.pls.csv.ParadoxCsvFileType"/>
    <lang.parserDefinition language="PARADOX_CSV" implementationClass="icu.windea.pls.csv.ParadoxCsvParserDefinition"/>

    <spellchecker.support language="PARADOX_CSV" implementationClass="icu.windea.pls.csv.editor.ParadoxCsvSpellcheckerStrategy"/>
    <lang.syntaxHighlighterFactory language="PARADOX_CSV" implementationClass="icu.windea.pls.csv.editor.ParadoxCsvSyntaxHighlighterFactory"/>
    <colorSettingsPage implementation="icu.windea.pls.csv.editor.ParadoxCsvColorSettingsPage"/>
    <lang.commenter language="PARADOX_CSV" implementationClass="icu.windea.pls.csv.editor.ParadoxCsvCommenter"/>
    <lang.findUsagesProvider language="PARADOX_CSV" implementationClass="icu.windea.pls.csv.editor.ParadoxCsvFindUsagesProvider"/>
    <elementDescriptionProvider implementation="icu.windea.pls.csv.editor.ParadoxCsvFindUsagesProvider"/>
    <extendWordSelectionHandler implementation="icu.windea.pls.csv.editor.ParadoxCsvWordSelectionHandler"/>
    <annotator language="PARADOX_CSV" implementationClass="icu.windea.pls.csv.editor.ParadoxCsvBaseAnnotator"/>
    <lang.foldingBuilder language="PARADOX_CSV" implementationClass="icu.windea.pls.csv.folding.ParadoxCsvFoldingBuilder"/>
    <declarationRangeHandler key="icu.windea.pls.csv.psi.ParadoxCsvRow" implementationClass="icu.windea.pls.csv.codeInsight.ParadoxCsvDeclarationRangeHandler"/>
    <highlightUsagesHandlerFactory implementation="icu.windea.pls.csv.codeInsight.highlighting.ParadoxCsvBaseHighlightUsagesHandlerFactory"/>
    <lang.quoteHandler language="PARADOX_CSV" implementationClass="icu.windea.pls.csv.codeInsight.editorActions.ParadoxCsvQuoteHandler"/>
    <langCodeStyleSettingsProvider implementation="icu.windea.pls.csv.formatter.ParadoxCsvCodeStyleSettingsProvider"/>
    <navbar implementation="icu.windea.pls.csv.navigation.ParadoxCsvNavBar"/>
    <breadcrumbsInfoProvider implementation="icu.windea.pls.csv.navigation.ParadoxCsvBreadCrumbsProvider"/>
    <lang.psiStructureViewFactory language="PARADOX_CSV" implementationClass="icu.windea.pls.csv.structureView.ParadoxCsvStructureViewFactory"/>

    <lang.elementManipulator forClass="icu.windea.pls.csv.psi.ParadoxCsvColumn"
                             implementationClass="icu.windea.pls.csv.psi.ParadoxCsvColumnManipulator"/>
  </extensions>
</idea-plugin>
