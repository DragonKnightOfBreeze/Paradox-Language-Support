<idea-plugin>
  <extensions defaultExtensionNs="com.intellij">
    <projectConfigurable id="pls.diagram" parentId="pls" instance="icu.windea.pls.extension.diagram.settings.ParadoxDiagramSettingsConfigurable"/>
    <projectConfigurable id="pls.diagram.Stellaris.EventTree" parentId="pls.diagram" instance="icu.windea.pls.extension.diagram.settings.impl.StellarisEventTreeDiagramSettingsConfigurable"/>
    <projectConfigurable id="pls.diagram.Stellaris.TechnologyTree" parentId="pls.diagram" instance="icu.windea.pls.extension.diagram.settings.impl.StellarisTechnologyTreeDiagramSettingsConfigurable"/>
    <projectConfigurable id="pls.diagram.Ck2.EventTree" parentId="pls.diagram" instance="icu.windea.pls.extension.diagram.settings.impl.Ck2EventTreeDiagramSettingsConfigurable"/>
    <projectConfigurable id="pls.diagram.Ck3.EventTree" parentId="pls.diagram" instance="icu.windea.pls.extension.diagram.settings.impl.Ck3EventTreeDiagramSettingsConfigurable"/>
    <projectConfigurable id="pls.diagram.Eu4.EventTree" parentId="pls.diagram" instance="icu.windea.pls.extension.diagram.settings.impl.Eu4EventTreeDiagramSettingsConfigurable"/>
    <projectConfigurable id="pls.diagram.Hoi4.EventTree" parentId="pls.diagram" instance="icu.windea.pls.extension.diagram.settings.impl.Hoi4EventTreeDiagramSettingsConfigurable"/>
    <projectConfigurable id="pls.diagram.Ir.EventTree" parentId="pls.diagram" instance="icu.windea.pls.extension.diagram.settings.impl.IrEventTreeDiagramSettingsConfigurable"/>
    <projectConfigurable id="pls.diagram.Vic2.EventTree" parentId="pls.diagram" instance="icu.windea.pls.extension.diagram.settings.impl.Vic2EventTreeDiagramSettingsConfigurable"/>
    <projectConfigurable id="pls.diagram.Vic3.EventTree" parentId="pls.diagram" instance="icu.windea.pls.extension.diagram.settings.impl.Vic3EventTreeDiagramSettingsConfigurable"/>
  </extensions>

  <extensions defaultExtensionNs="com.intellij.diagram">
    <Provider implementation="icu.windea.pls.extension.diagram.provider.impl.StellarisEventTreeDiagramProvider"/>
    <Provider implementation="icu.windea.pls.extension.diagram.provider.impl.StellarisTechnologyTreeDiagramProvider"/>
    <Provider implementation="icu.windea.pls.extension.diagram.provider.impl.Ck2EventTreeDiagramProvider"/>
    <Provider implementation="icu.windea.pls.extension.diagram.provider.impl.Ck3EventTreeDiagramProvider"/>
    <Provider implementation="icu.windea.pls.extension.diagram.provider.impl.Eu4EventTreeDiagramProvider"/>
    <Provider implementation="icu.windea.pls.extension.diagram.provider.impl.Hoi4EventTreeDiagramProvider"/>
    <Provider implementation="icu.windea.pls.extension.diagram.provider.impl.IrEventTreeDiagramProvider"/>
    <Provider implementation="icu.windea.pls.extension.diagram.provider.impl.Vic2EventTreeDiagramProvider"/>
    <Provider implementation="icu.windea.pls.extension.diagram.provider.impl.Vic3EventTreeDiagramProvider"/>
  </extensions>

  <actions>
    <reference id="Pls.GotoGroup">
      <add-to-group group-id="Uml.SourceActionsGroup.GoTo" relative-to-action="GotoRelated" anchor="after"/>
      <add-to-group group-id="Uml.NodeCellEditorPopup.GoTo" relative-to-action="GotoRelated" anchor="after"/>
    </reference>
    <action id="Pls.Diagram.OpenSettings"
            class="icu.windea.pls.extension.diagram.actions.ParadoxDiagramOpenSettingsAction"
            icon="AllIcons.General.GearPlain">
      <add-to-group group-id="Diagram.DefaultGraphToolbar" relative-to-action="Diagram.OpenSettings" anchor="after"/>
    </action>
    <reference id="UML.Group">
      <add-to-group group-id="Pls.DefinitionHierarchyPopupMenu"/>
      <add-to-group group-id="Pls.CallHierarchyPopupMenu"/>
    </reference>
  </actions>
</idea-plugin>
