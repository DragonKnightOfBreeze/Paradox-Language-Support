<idea-plugin>
  <extensionPoints>
    <extensionPoint qualifiedName="icu.windea.pls.tools.modImporter" interface="icu.windea.pls.tools.importer.ParadoxModImporter" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.tools.modExporter" interface="icu.windea.pls.tools.exporter.ParadoxModExporter" dynamic="true"/>
  </extensionPoints>

  <extensions defaultExtensionNs="icu.windea.pls.tools">
    <modImporter implementation="icu.windea.pls.tools.importer.ParadoxFromGameImporter"/>
    <!--<modImporter implementation="icu.windea.pls.tools.importer.ParadoxFromLauncherImporter"/>-->
    <!--<modImporter implementation="icu.windea.pls.tools.importer.ParadoxFromLauncherBetaImporter"/>-->
    <modImporter implementation="icu.windea.pls.tools.importer.ParadoxFromLauncherJsonV3Importer"/>

    <modExporter implementation="icu.windea.pls.tools.exporter.ParadoxToLauncherJsonV2Exporter"/>
    <modExporter implementation="icu.windea.pls.tools.exporter.ParadoxToLauncherJsonV3Exporter"/>
  </extensions>

  <actions>
    <group id="Pls.Tools" class="icu.windea.pls.tools.actions.PlsToolsActionGroup" popup="true">
      <add-to-group group-id="ToolsMenu"/>
      <add-to-group group-id="EditorPopupMenu"/>
      <add-to-group group-id="ProjectViewPopupMenu"/>

      <add-to-group group-id="Images.EditorPopupMenu"/>
      <add-to-group group-id="Pls.Images.EditorPopupMenu"/>
    </group>

    <action id="Pls.OpenGameSettings" class="icu.windea.pls.tools.actions.OpenGameSettingsAction" icon="icu.windea.pls.PlsIcons.Actions.GameSettings">
      <keyboard-shortcut keymap="$default" first-keystroke="control alt m"/>
      <add-to-group group-id="Pls.Tools" anchor="first"/>
      <add-to-group group-id="EditorContextBarMenu" anchor="first"/>
    </action>
    <action id="Pls.OpenModSettings" class="icu.windea.pls.tools.actions.OpenModSettingsAction" icon="icu.windea.pls.PlsIcons.Actions.ModSettings">
      <keyboard-shortcut keymap="$default" first-keystroke="control alt m"/>
      <add-to-group group-id="Pls.Tools" relative-to-action="Pls.OpenGameSettings" anchor="after"/>
      <add-to-group group-id="EditorContextBarMenu" relative-to-action="Pls.OpenGameSettings" anchor="after"/>
    </action>

    <group id="Pls.OpenPath" popup="true">
      <action id="Pls.OpenPath.OpenSteamPath" class="icu.windea.pls.tools.actions.OpenPathActions$Steam"/>
      <action id="Pls.OpenPath.OpenSteamGamePath" class="icu.windea.pls.tools.actions.OpenPathActions$SteamGame"/>
      <action id="Pls.OpenPath.OpenSteamWorkshopPath" class="icu.windea.pls.tools.actions.OpenPathActions$SteamWorkshop"/>
      <action id="Pls.OpenPath.OpenGameDataPath" class="icu.windea.pls.tools.actions.OpenPathActions$GameData"/>
      <action id="Pls.OpenPath.OpenGamePath" class="icu.windea.pls.tools.actions.OpenPathActions$Game"/>
      <action id="Pls.OpenPath.OpenModPath" class="icu.windea.pls.tools.actions.OpenPathActions$Mod"/>
      <add-to-group group-id="Pls.Tools"/>
    </group>
    <group id="Pls.OpenUrl" popup="true">
      <action id="Pls.OpenUrl.OpenGameStorePageInSteam" class="icu.windea.pls.tools.actions.OpenUrlActions$GameStorePageInSteam" icon="icu.windea.pls.PlsIcons.General.Steam"/>
      <action id="Pls.OpenUrl.OpenGameStorePageInSteamWebsite" class="icu.windea.pls.tools.actions.OpenUrlActions$GameStorePageInSteamWebsite" icon="icu.windea.pls.PlsIcons.General.Steam"/>
      <action id="Pls.OpenUrl.OpenGameWorkshopPageInSteam" class="icu.windea.pls.tools.actions.OpenUrlActions$GameWorkshopPageInSteam" icon="icu.windea.pls.PlsIcons.General.Steam"/>
      <action id="Pls.OpenUrl.OpenGameWorkshopPageInSteamWebsite" class="icu.windea.pls.tools.actions.OpenUrlActions$GameWorkshopPageInSteamWebsite" icon="icu.windea.pls.PlsIcons.General.Steam"/>
      <action id="Pls.OpenUrl.OpenModPageInSteam" class="icu.windea.pls.tools.actions.OpenUrlActions$ModPageInSteam" icon="icu.windea.pls.PlsIcons.General.Steam"/>
      <action id="Pls.OpenUrl.OpenModPageInSteamWebsite" class="icu.windea.pls.tools.actions.OpenUrlActions$ModPageInSteamWebsite" icon="icu.windea.pls.PlsIcons.General.Steam"/>
      <add-to-group group-id="Pls.Tools"/>
    </group>
    <group id="Pls.CopyPath" popup="true">
      <action id="Pls.CopyPath.CopySteamPath" class="icu.windea.pls.tools.actions.CopyPathActions$Steam"/>
      <action id="Pls.CopyPath.CopySteamGamePath" class="icu.windea.pls.tools.actions.CopyPathActions$SteamGame"/>
      <action id="Pls.CopyPath.CopySteamWorkshopPath" class="icu.windea.pls.tools.actions.CopyPathActions$SteamWorkshop"/>
      <action id="Pls.CopyPath.CopyGameDataPath" class="icu.windea.pls.tools.actions.CopyPathActions$GameData"/>
      <action id="Pls.CopyPath.CopyGamePath" class="icu.windea.pls.tools.actions.CopyPathActions$Game"/>
      <action id="Pls.CopyPath.CopyModPath" class="icu.windea.pls.tools.actions.CopyPathActions$Mod"/>
      <add-to-group group-id="Pls.Tools"/>
    </group>
    <group id="Pls.CopyUrl" popup="true">
      <action id="Pls.CopyUrl.CopyGameStorePageUrl" class="icu.windea.pls.tools.actions.CopyUrlActions$GameStorePage" icon="icu.windea.pls.PlsIcons.General.Steam"/>
      <action id="Pls.CopyUrl.CopyGameWorkshopPageUrl" class="icu.windea.pls.tools.actions.CopyUrlActions$GameWorkshopPage" icon="icu.windea.pls.PlsIcons.General.Steam"/>
      <action id="Pls.CopyUrl.CopyModPageUrl" class="icu.windea.pls.tools.actions.CopyUrlActions$ModPage" icon="icu.windea.pls.PlsIcons.General.Steam"/>
      <add-to-group group-id="Pls.Tools"/>
    </group>

    <group id="Pls.GotoPath">
      <action id="Pls.FileChooser.GoToSteamPath" class="icu.windea.pls.tools.actions.GoToPathActions$Steam" icon="AllIcons.Nodes.Module"/>
      <action id="Pls.FileChooser.GoToSteamGamePath" class="icu.windea.pls.tools.actions.GoToPathActions$SteamGame" icon="AllIcons.Nodes.Module"/>
      <action id="Pls.FileChooser.GoToSteamWorkshopPath" class="icu.windea.pls.tools.actions.GoToPathActions$SteamWorkshop" icon="AllIcons.Nodes.Module"/>
      <action id="Pls.FileChooser.GoToGameDataPath" class="icu.windea.pls.tools.actions.GoToPathActions$GameData" icon="AllIcons.Nodes.Module"/>
      <add-to-group group-id="Pls.Tools"/>
    </group>
    <group id="Pls.GotoPath.WithSeparator">
      <separator/>
      <reference ref="Pls.GotoPath"/>
      <add-to-group group-id="FileChooserToolbar" anchor="last"/>
    </group>

    <group id="Pls.Generate" popup="true">
      <action id="Pls.Generate.GenerateLocalisationFile" class="icu.windea.pls.tools.actions.GenerateLocalisationFileAction"/>
      <add-to-group group-id="Pls.Tools"/>
    </group>
  </actions>
</idea-plugin>
