<idea-plugin>
  <extensionPoints>
    <extensionPoint qualifiedName="icu.windea.pls.configExpressionReplacer" interface="icu.windea.pls.config.cwt.replacer.CwtConfigExpressionReplacer" dynamic="true"/>
    
    <extensionPoint qualifiedName="icu.windea.pls.definitionExtendedDocumentationProvider" interface="icu.windea.pls.lang.documentation.ParadoxDefinitionExtendedDocumentationProvider" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.pathReferenceExpression" interface="icu.windea.pls.lang.expression.ParadoxPathReferenceExpression" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.scriptMemberElementInlineSupport" interface="icu.windea.pls.lang.support.ParadoxScriptMemberElementInlineSupport" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.colorSupport" interface="icu.windea.pls.lang.support.ParadoxColorSupport" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.parameterSupport" interface="icu.windea.pls.lang.support.ParadoxParameterSupport" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.modifierSupport" interface="icu.windea.pls.lang.support.ParadoxModifierSupport" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.inferredScopeContextProvider" interface="icu.windea.pls.lang.provider.ParadoxInferredScopeContextProvider" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.definitionPresentationProvider" interface="icu.windea.pls.lang.presentation.ParadoxDefinitionPresentationProvider" dynamic="true"/>
    
    <extensionPoint qualifiedName="icu.windea.pls.modImporter" interface="icu.windea.pls.core.tool.importer.ParadoxModImporter" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.modExporter" interface="icu.windea.pls.core.tool.exporter.ParadoxModExporter" dynamic="true"/>
  </extensionPoints>

  <extensions defaultExtensionNs="icu.windea.pls">
    <configExpressionReplacer implementation="icu.windea.pls.config.cwt.replacer.CwtOnActionConfigExpressionReplacer"/>
    
    <definitionExtendedDocumentationProvider implementation="icu.windea.pls.lang.documentation.ParadoxOnActionExtendedDocumentationProvider"/>
    <definitionExtendedDocumentationProvider implementation="icu.windea.pls.lang.documentation.ParadoxBaseDefinitionExtendedDocumentationProvider"/>
    
    <pathReferenceExpression implementation="icu.windea.pls.lang.expression.ParadoxIconPathReferenceExpression"/>
    <pathReferenceExpression implementation="icu.windea.pls.lang.expression.ParadoxFilePathPathReferenceExpression"/>
    <pathReferenceExpression implementation="icu.windea.pls.lang.expression.ParadoxFileNamePathReferenceExpression"/>
    
    <scriptMemberElementInlineSupport implementation="icu.windea.pls.lang.support.ParadoxInlineScriptScriptMemberElementInlineSupport"/>
    
    <colorSupport implementation="icu.windea.pls.lang.support.ParadoxScriptColorColorSupport"/>
    <colorSupport implementation="icu.windea.pls.lang.support.ParadoxScriptBlockColorSupport"/>
    <colorSupport implementation="icu.windea.pls.lang.support.ParadoxScriptStringColorSupport"/>
    
    <parameterSupport implementation="icu.windea.pls.lang.support.ParadoxDefinitionParameterSupport"/>
    <parameterSupport implementation="icu.windea.pls.lang.support.ParadoxInlineScriptParameterSupport"/>
    
    <modifierSupport implementation="icu.windea.pls.lang.support.ParadoxTemplateModifierSupport"/>
    <modifierSupport implementation="icu.windea.pls.lang.support.StellarisEconomicCategoryModifierSupport"/>
    
    <inferredScopeContextProvider implementation="icu.windea.pls.lang.provider.ParadoxEventFromOnActionInferredScopeContextProvider"/>
    
    <definitionPresentationProvider implementation="icu.windea.pls.lang.presentation.StellarisTechnologyPresentationProvider"/>
    
    <modImporter implementation="icu.windea.pls.core.tool.importer.ParadoxFromGameImporter"/>
    <!--<modImporter implementation="icu.windea.pls.core.tool.importer.ParadoxFromLauncherImporter"/>-->
    <!--<modImporter implementation="icu.windea.pls.core.tool.importer.ParadoxFromLauncherBetaImporter"/>-->
    <modImporter implementation="icu.windea.pls.core.tool.importer.ParadoxFromLauncherJsonV3Importer"/>

    <modExporter implementation="icu.windea.pls.core.tool.exporter.ParadoxToLauncherJsonV2Exporter"/>
    <modExporter implementation="icu.windea.pls.core.tool.exporter.ParadoxToLauncherJsonV3Exporter"/>
  </extensions>
</idea-plugin>
