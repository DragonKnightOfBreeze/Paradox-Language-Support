<idea-plugin>
  <extensionPoints>
    <extensionPoint qualifiedName="icu.windea.pls.configExpressionReplacer" interface="icu.windea.pls.lang.config.CwtConfigExpressionReplacer" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.definitionExtendedDocumentationProvider" interface="icu.windea.pls.lang.documentation.ParadoxDefinitionExtendedDocumentationProvider" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.scriptExpressionSupport" interface="icu.windea.pls.lang.expression.ParadoxScriptExpressionSupport" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.pathReferenceExpressionSupport"  interface="icu.windea.pls.lang.expression.ParadoxPathReferenceExpressionSupport" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.scriptMemberElementInlineSupport" interface="icu.windea.pls.lang.inline.ParadoxScriptMemberElementInlineSupport" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.colorSupport" interface="icu.windea.pls.lang.color.ParadoxColorSupport" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.parameterSupport" interface="icu.windea.pls.lang.parameter.ParadoxParameterSupport" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.modifierSupport" interface="icu.windea.pls.lang.modifier.ParadoxModifierSupport" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.inferredScopeContextProvider" interface="icu.windea.pls.lang.scope.ParadoxInferredScopeContextProvider" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.definitionPresentationProvider" interface="icu.windea.pls.lang.presentation.ParadoxDefinitionPresentationProvider" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.definitionDataProvider" interface="icu.windea.pls.lang.data.ParadoxDefinitionDataProvider" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.modImporter" interface="icu.windea.pls.core.tool.importer.ParadoxModImporter" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.modExporter" interface="icu.windea.pls.core.tool.exporter.ParadoxModExporter" dynamic="true"/>
  </extensionPoints>

  <extensions defaultExtensionNs="icu.windea.pls">
    <configExpressionReplacer implementation="icu.windea.pls.lang.config.CwtOnActionConfigExpressionReplacer"/>
    
    <definitionExtendedDocumentationProvider implementation="icu.windea.pls.lang.documentation.ParadoxOnActionExtendedDocumentationProvider"/>
    <definitionExtendedDocumentationProvider implementation="icu.windea.pls.lang.documentation.ParadoxBaseDefinitionExtendedDocumentationProvider"/>
    
    <pathReferenceExpressionSupport implementation="icu.windea.pls.lang.expression.ParadoxIconReferenceExpressionSupport"/>
    <pathReferenceExpressionSupport implementation="icu.windea.pls.lang.expression.ParadoxFilePathReferenceExpressionSupport"/>
    <pathReferenceExpressionSupport implementation="icu.windea.pls.lang.expression.ParadoxFileNameReferenceExpressionSupport"/>
    
    <scriptExpressionSupport implementation="icu.windea.pls.lang.expression.ParadoxScriptBlockExpressionSupport"/>
    <scriptExpressionSupport implementation="icu.windea.pls.lang.expression.ParadoxScriptBoolExpressionSupport"/>
    <scriptExpressionSupport implementation="icu.windea.pls.lang.expression.ParadoxScriptLocalisationExpressionSupport"/>
    <scriptExpressionSupport implementation="icu.windea.pls.lang.expression.ParadoxScriptSyncedLocalisationExpressionSupport"/>
    <scriptExpressionSupport implementation="icu.windea.pls.lang.expression.ParadoxScriptInlineLocalisationExpressionSupport"/>
    <scriptExpressionSupport implementation="icu.windea.pls.lang.expression.ParadoxScriptDefinitionExpressionSupport"/>
    <scriptExpressionSupport implementation="icu.windea.pls.lang.expression.ParadoxScriptPathReferenceExpressionSupport"/>
    <scriptExpressionSupport implementation="icu.windea.pls.lang.expression.ParadoxScriptEnumValueExpressionSupport"/>
    <scriptExpressionSupport implementation="icu.windea.pls.lang.expression.ParadoxScriptModifierExpressionSupport"/>
    <scriptExpressionSupport implementation="icu.windea.pls.lang.expression.ParadoxScriptParameterExpressionSupport"/>
    <scriptExpressionSupport implementation="icu.windea.pls.lang.expression.ParadoxScriptAliasNameExpressionSupport"/>
    <scriptExpressionSupport implementation="icu.windea.pls.lang.expression.ParadoxScriptConstantExpressionSupport"/>
    <scriptExpressionSupport implementation="icu.windea.pls.lang.expression.ParadoxScriptTemplateExpressionSupport"/>
    
    <scriptMemberElementInlineSupport implementation="icu.windea.pls.lang.inline.ParadoxInlineScriptScriptMemberElementInlineSupport"/>
    
    <colorSupport implementation="icu.windea.pls.lang.color.ParadoxScriptColorColorSupport"/>
    <colorSupport implementation="icu.windea.pls.lang.color.ParadoxScriptBlockColorSupport"/>
    <colorSupport implementation="icu.windea.pls.lang.color.ParadoxScriptStringColorSupport"/>
    
    <parameterSupport implementation="icu.windea.pls.lang.parameter.ParadoxDefinitionParameterSupport"/>
    <parameterSupport implementation="icu.windea.pls.lang.parameter.ParadoxInlineScriptParameterSupport"/>
    
    <modifierSupport implementation="icu.windea.pls.lang.modifier.ParadoxTemplateModifierSupport"/>
    <modifierSupport implementation="icu.windea.pls.lang.modifier.StellarisEconomicCategoryModifierSupport"/>
    
    <inferredScopeContextProvider implementation="icu.windea.pls.lang.scope.ParadoxEventFromOnActionInferredScopeContextProvider"/>
    
    <definitionPresentationProvider implementation="icu.windea.pls.lang.presentation.StellarisTechnologyPresentationProvider"/>

    <definitionDataProvider implementation="icu.windea.pls.lang.data.ParadoxEventDataProvider"/>/
    <definitionDataProvider implementation="icu.windea.pls.lang.data.StellarisTechnologyDataProvider"/>/
    
    <modImporter implementation="icu.windea.pls.core.tool.importer.ParadoxFromGameImporter"/>
    <!--<modImporter implementation="icu.windea.pls.core.tool.importer.ParadoxFromLauncherImporter"/>-->
    <!--<modImporter implementation="icu.windea.pls.core.tool.importer.ParadoxFromLauncherBetaImporter"/>-->
    <modImporter implementation="icu.windea.pls.core.tool.importer.ParadoxFromLauncherJsonV3Importer"/>

    <modExporter implementation="icu.windea.pls.core.tool.exporter.ParadoxToLauncherJsonV2Exporter"/>
    <modExporter implementation="icu.windea.pls.core.tool.exporter.ParadoxToLauncherJsonV3Exporter"/>
  </extensions>
</idea-plugin>
