<idea-plugin>
  <extensionPoints>
    <extensionPoint qualifiedName="icu.windea.pls.inject.codeInjectorSupport" interface="icu.windea.pls.inject.CodeInjectorSupport" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.inject.codeInjector" interface="icu.windea.pls.inject.CodeInjector" dynamic="true"/>
  </extensionPoints>

  <applicationListeners>
    <listener class="icu.windea.pls.inject.CodeInjectorService$Listener" topic="com.intellij.ide.AppLifecycleListener"/>
  </applicationListeners>

  <extensions defaultExtensionNs="icu.windea.pls.inject">
    <codeInjectorSupport implementation="icu.windea.pls.inject.support.BaseCodeInjectorSupport"/>
    <codeInjectorSupport implementation="icu.windea.pls.inject.support.FieldCacheCodeInjectorSupport"/>
    <codeInjectorSupport implementation="icu.windea.pls.inject.support.OptimizedFieldCodeInjectorSupport"/>
    <codeInjectorSupport implementation="icu.windea.pls.inject.support.InlinedDelegateFieldCodeInjectorSupport"/>
  </extensions>

  <extensions defaultExtensionNs="icu.windea.pls.inject">
    <!--core code injectors-->
    <codeInjector implementation="icu.windea.pls.inject.injectors.LowLevelSearchUtilCodeInjector"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.RefManagerImplCodeInjector"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.LineCommentCopyPastePreProcessorCodeInjector"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.CommentByLineCommentHandlerCodeInjector"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.PathReferenceManagerImplCodeInjector"/>

    <!--code injectors to provide additional features-->
    <codeInjector implementation="icu.windea.pls.inject.injectors.FileRenderCodeInjector"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.InjectionRegistrarImplCodeInjector"/>

    <!--code injectors to optimize image readers (delegate to image tools)-->
    <codeInjector implementation="icu.windea.pls.inject.injectors.DDSImageReaderCodeInjector"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.TGAImageReaderCodeInjector"/>

    <!--code injectors to fix bugs-->
    <codeInjector implementation="icu.windea.pls.inject.injectors.fix.SymbolNavigationServiceImplCodeInjector"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.fix.InjectLanguageActionCodeInjector"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.fix.WithAttributesPresentationCodeInjector"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.fix.IdentifierHighlightingManagerImplCodeInjector"/>

    <!--code injectors to optimize memory-->
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.OptimizedFieldCodeInjectors$BasePresentation"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.InlinedDelegateFieldCodeInjectors$CwtOptionDataHolderBase"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.InlinedDelegateFieldCodeInjectors$CwtConfigGroupDataHolderBase"/>

    <!--code injectors to optimize performance-->
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.CwtPsiCodeInjectors$Property"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.CwtPsiCodeInjectors$PropertyKey"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.CwtPsiCodeInjectors$Option"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.CwtPsiCodeInjectors$OptionKey"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.CwtPsiCodeInjectors$Boolean"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.CwtPsiCodeInjectors$Int"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.CwtPsiCodeInjectors$Float"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.CwtPsiCodeInjectors$String"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxScriptPsiCodeInjectors$ScriptedVariable"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxScriptPsiCodeInjectors$ScriptedVariableName"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxScriptPsiCodeInjectors$Property"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxScriptPsiCodeInjectors$PropertyKey"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxScriptPsiCodeInjectors$Boolean"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxScriptPsiCodeInjectors$Int"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxScriptPsiCodeInjectors$Float"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxScriptPsiCodeInjectors$String"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxScriptPsiCodeInjectors$Color"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxScriptPsiCodeInjectors$InlineMath"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxScriptPsiCodeInjectors$ParameterConditionParameter"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxScriptPsiCodeInjectors$Parameter"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxScriptPsiCodeInjectors$InlineMathParameter"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxLocalisationPsiCodeInjectors$Locale"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxLocalisationPsiCodeInjectors$Property"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxLocalisationPsiCodeInjectors$PropertyKey"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxLocalisationPsiCodeInjectors$PropertyValue"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxLocalisationPsiCodeInjectors$Parameter"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxLocalisationPsiCodeInjectors$Icon"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxLocalisationPsiCodeInjectors$TextFormat"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxLocalisationPsiCodeInjectors$TextIcon"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.addon.ParadoxCsvPsiCodeInjectors$Column"/>
  </extensions>

  <extensionPoints>
    <extensionPoint qualifiedName="icu.windea.pls.inject.injectedFileProcessor" interface="icu.windea.pls.inject.processors.InjectedFileProcessor" dynamic="true"/>
  </extensionPoints>
</idea-plugin>
