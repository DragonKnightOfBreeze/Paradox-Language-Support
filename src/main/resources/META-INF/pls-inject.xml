<idea-plugin>
  <extensionPoints>
    <extensionPoint qualifiedName="icu.windea.pls.inject.codeInjectorSupport" interface="icu.windea.pls.inject.CodeInjectorSupport" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.inject.codeInjector" interface="icu.windea.pls.inject.CodeInjector" dynamic="true"/>
  </extensionPoints>

  <applicationListeners>
    <listener class="icu.windea.pls.inject.CodeInjectorService$Listener" topic="com.intellij.ide.AppLifecycleListener"/>
  </applicationListeners>

  <extensions defaultExtensionNs="icu.windea.pls.inject">
    <codeInjectorSupport implementation="icu.windea.pls.inject.support.BaseCodeInjectorSupport"/>
    <codeInjectorSupport implementation="icu.windea.pls.inject.support.FieldCacheCodeInjectorSupport"/>
    <codeInjectorSupport implementation="icu.windea.pls.inject.support.OptimizedFieldCodeInjectorSupport"/>
    <codeInjectorSupport implementation="icu.windea.pls.inject.support.InlinedDelegateFieldCodeInjectorSupport"/>
  </extensions>

  <extensions defaultExtensionNs="icu.windea.pls.inject">
    <!--core code injectors-->
    <codeInjector implementation="icu.windea.pls.inject.injectors.LowLevelSearchUtilCodeInjector"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.RefManagerImplCodeInjector"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.SymbolNavigationServiceImplCodeInjector"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.LineCommentCopyPastePreProcessorCodeInjector"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.CommentByLineCommentHandlerCodeInjector"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.PathReferenceManagerImplCodeInjector"/>

    <!--code injectors to optimize image readers (delegate to image tools)-->
    <codeInjector implementation="icu.windea.pls.inject.injectors.DDSImageReaderCodeInjector"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.TGAImageReaderCodeInjector"/>

    <!--code injectors to fix bugs-->
    <codeInjector implementation="icu.windea.pls.inject.injectors.InjectLanguageActionCodeInjector"/>
    <!--<codeInjector implementation="icu.windea.pls.inject.injectors.ImageDescriptorKtCodeInjector"/>-->

    <!--code injectors to provide additional features-->
    <codeInjector implementation="icu.windea.pls.inject.injectors.FileRenderCodeInjector"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.InjectionRegistrarImplCodeInjector"/>

    <!--code injectors to optimize memory-->
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.OptimizedFieldCodeInjectors$BasePresentation"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.InlinedDelegateFieldCodeInjectors$CwtOptionDataHolderBase"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.InlinedDelegateFieldCodeInjectors$CwtConfigGroupDataHolderBase"/>

    <!--code injectors to optimize performance-->
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.CwtPsiCodeInjectors$Property"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.CwtPsiCodeInjectors$PropertyKey"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.CwtPsiCodeInjectors$Option"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.CwtPsiCodeInjectors$OptionKey"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.CwtPsiCodeInjectors$Boolean"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.CwtPsiCodeInjectors$Int"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.CwtPsiCodeInjectors$Float"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.CwtPsiCodeInjectors$String"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxScriptPsiCodeInjectors$ScriptedVariable"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxScriptPsiCodeInjectors$ScriptedVariableName"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxScriptPsiCodeInjectors$Property"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxScriptPsiCodeInjectors$PropertyKey"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxScriptPsiCodeInjectors$Boolean"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxScriptPsiCodeInjectors$Int"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxScriptPsiCodeInjectors$Float"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxScriptPsiCodeInjectors$String"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxScriptPsiCodeInjectors$Color"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxScriptPsiCodeInjectors$InlineMath"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxScriptPsiCodeInjectors$ParameterConditionParameter"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxScriptPsiCodeInjectors$Parameter"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxScriptPsiCodeInjectors$InlineMathParameter"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxLocalisationPsiCodeInjectors$Locale"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxLocalisationPsiCodeInjectors$Property"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxLocalisationPsiCodeInjectors$PropertyKey"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxLocalisationPsiCodeInjectors$PropertyValue"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxLocalisationPsiCodeInjectors$Parameter"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxLocalisationPsiCodeInjectors$Icon"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxLocalisationPsiCodeInjectors$TextFormat"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxLocalisationPsiCodeInjectors$TextIcon"/>
    <codeInjector implementation="icu.windea.pls.inject.injectors.ext.ParadoxCsvPsiCodeInjectors$Column"/>
  </extensions>

  <extensionPoints>
    <extensionPoint qualifiedName="icu.windea.pls.inject.injectedFileProcessor" interface="icu.windea.pls.inject.processors.InjectedFileProcessor" dynamic="true"/>
  </extensionPoints>
</idea-plugin>
