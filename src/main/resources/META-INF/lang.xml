    <paradoxPathReferenceExpression implementation="icu.windea.pls.lang.expression.ParadoxFilePathPathReferenceExpression"/>
    <paradoxPathReferenceExpression implementation="icu.windea.pls.lang.expression.ParadoxFileNamePathReferenceExpression"/>
    
    <paradoxMemberElementLinker implementation="icu.windea.pls.lang.linker.ParadoxInlineScriptMemberElementLinker"/>
    
