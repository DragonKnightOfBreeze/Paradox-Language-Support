<idea-plugin>
  <extensionPoints>
    <extensionPoint qualifiedName="icu.windea.pls.paradoxMemberElementLinker" interface="icu.windea.pls.lang.linker.ParadoxMemberElementLinker" dynamic="true"/>
    
    <extensionPoint qualifiedName="icu.windea.pls.paradoxParameterSupport" interface="icu.windea.pls.lang.support.ParadoxParameterSupport" dynamic="true"/>
    <extensionPoint qualifiedName="icu.windea.pls.paradoxModifierSupport" interface="icu.windea.pls.lang.support.ParadoxModifierSupport" dynamic="true"/>
  </extensionPoints>

  <extensions defaultExtensionNs="icu.windea.pls">
    <paradoxMemberElementLinker implementation="icu.windea.pls.lang.linker.ParadoxInlineScriptMemberElementLinker"/>
    
    <paradoxParameterSupport implementation="icu.windea.pls.lang.support.ParadoxDefinitionParameterSupport"/>
    <paradoxParameterSupport implementation="icu.windea.pls.lang.support.ParadoxInlineScriptParameterSupport"/>
    <paradoxModifierSupport implementation="icu.windea.pls.lang.support.ParadoxTemplateModifierSupport"/>
    <paradoxModifierSupport implementation="icu.windea.pls.lang.support.ParadoxEconomicCategoryModifierSupport"/>
  </extensions>
</idea-plugin>
