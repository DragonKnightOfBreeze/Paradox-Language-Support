    <paradoxColorSupport implementation="icu.windea.pls.lang.support.ParadoxScriptBlockColorSupport"/>
    <paradoxColorSupport implementation="icu.windea.pls.lang.support.ParadoxScriptStringColorSupport"/>
    
    <paradoxParameterSupport implementation="icu.windea.pls.lang.support.ParadoxDefinitionParameterSupport"/>
    <paradoxParameterSupport implementation="icu.windea.pls.lang.support.ParadoxInlineScriptParameterSupport"/>
    
    <paradoxModifierSupport implementation="icu.windea.pls.lang.support.ParadoxTemplateModifierSupport"/>
    <paradoxModifierSupport implementation="icu.windea.pls.lang.support.ParadoxEconomicCategoryModifierSupport"/>
  </extensions>
</idea-plugin>
