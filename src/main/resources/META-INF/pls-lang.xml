      <action id="Pls.DefinitionHierarchy.TechTreePre" class="icu.windea.pls.lang.hierarchy.type.ParadoxDefinitionHierarchyActions$ViewTechTreePreAction"/>
      <action id="Pls.DefinitionHierarchy.TechTreePost" class="icu.windea.pls.lang.hierarchy.type.ParadoxDefinitionHierarchyActions$ViewTechTreePostAction"/>
      <reference ref="HierarchyPopupMenu.Common"/>
    </group>
    <group id="Pls.CallHierarchyPopupMenu">
      <action id="Pls.CallHierarchy.BaseOnThis" class="icu.windea.pls.lang.hierarchy.call.ParadoxCallHierarchyBrowser$BaseOnThisAction"/>
      <reference ref="HierarchyPopupMenu.Common"/>
    </group>

    <group id="Pls.LocalisationManipulationContextMenuGroup" class="com.intellij.ide.actions.NonTrivialActionGroup" popup="true" compact="true"/>
    <group id="Pls.LocalisationGenerationContextMenuGroup" class="com.intellij.ide.actions.NonTrivialActionGroup" popup="true" compact="true"/>
    <group id="Pls.TableManipulationContextMenuGroup" class="com.intellij.ide.actions.NonTrivialActionGroup" popup="true" compact="true"/>

    <group id="Pls.EditorContextMenuGroup">
      <add-to-group group-id="EditorPopupMenu" anchor="after" relative-to-action="EditorToggleColumnMode" />
      <separator/>
      <reference ref="Pls.LocalisationManipulationContextMenuGroup"/>
      <reference ref="Pls.LocalisationGenerationContextMenuGroup"/>
      <reference ref="Pls.TableManipulationContextMenuGroup"/>
    </group>

    <group id="Pls.LocalisationManipulation">
      <action id="Pls.Manipulation.ReplaceLocalisationFromLocale" class="icu.windea.pls.lang.actions.localisation.ReplaceLocalisationFromLocaleAction"/>
      <action id="Pls.Manipulation.ReplaceLocalisationWithTranslation" class="icu.windea.pls.lang.actions.localisation.ReplaceLocalisationWithTranslationAction"/>
      <action id="Pls.Manipulation.ReplaceLocalisationWithTranslationFromLocale" class="icu.windea.pls.lang.actions.localisation.ReplaceLocalisationWithTranslationFromLocaleAction"/>
      <add-to-group group-id="Pls.LocalisationManipulationContextMenuGroup"/>
