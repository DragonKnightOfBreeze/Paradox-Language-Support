    <action id="Pls.OpenGameSettings" class="icu.windea.pls.lang.actions.tools.OpenSettingsActions$Game" icon="icu.windea.pls.PlsIcons.Actions.GameSettings">
      <keyboard-shortcut keymap="$default" first-keystroke="control alt m"/>
      <add-to-group group-id="EditorContextBarMenu" anchor="first"/>
    </action>
    <action id="Pls.OpenModSettings" class="icu.windea.pls.lang.actions.tools.OpenSettingsActions$Mod" icon="icu.windea.pls.PlsIcons.Actions.ModSettings">
      <keyboard-shortcut keymap="$default" first-keystroke="control alt m"/>
      <add-to-group group-id="EditorContextBarMenu" relative-to-action="Pls.OpenGameSettings" anchor="after"/>
    </action>

