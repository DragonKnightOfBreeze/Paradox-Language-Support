    <group id="Pls.Tools" class="icu.windea.pls.lang.actions.tools.ToolsActionGroup" popup="true">
      <reference ref="Pls.OpenGameSettings"/>
      <reference ref="Pls.OpenModSettings"/>
      <reference ref="Pls.SyncConfigGroupFromRemote"/>

      <group id="Pls.Tools.OpenPath" popup="true">
        <reference ref="Pls.OpenPath"/>
      </group>
      <group id="Pls.Tools.OpenUrl" popup="true">
        <reference ref="Pls.OpenUrl"/>
      </group>
      <group id="Pls.Tools.CopyPath" popup="true">
        <reference ref="Pls.CopyPath"/>
      </group>
      <group id="Pls.Tools.CopyUrl" popup="true">
        <reference ref="Pls.CopyUrl"/>
      </group>

      <group id="Pls.Tools.Manipulation" popup="true">
        <reference ref="Pls.LocalisationManipulation"/>
        <reference ref="Pls.TableManipulation"/>
      </group>
      <group id="Pls.Tools.Generation" popup="true">
        <reference ref="Pls.LocalisationGeneration"/>
      </group>

      <add-to-group group-id="ToolsMenu"/>
      <add-to-group group-id="EditorPopupMenu"/>
      <add-to-group group-id="ProjectViewPopupMenu"/>
      <add-to-group group-id="Images.EditorPopupMenu"/>
      <add-to-group group-id="Pls.Images.EditorPopupMenu"/>
    </group>
