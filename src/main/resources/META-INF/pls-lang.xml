      <action id="Pls.OpenUrl.GameStorePageInSteamWebsite" class="icu.windea.pls.lang.actions.tools.OpenUrlActions$GameStorePageInSteamWebsite" icon="icu.windea.pls.PlsIcons.General.Steam"/>
      <action id="Pls.OpenUrl.GameWorkshopPageInSteam" class="icu.windea.pls.lang.actions.tools.OpenUrlActions$GameWorkshopPageInSteam" icon="icu.windea.pls.PlsIcons.General.Steam"/>
      <action id="Pls.OpenUrl.GameWorkshopPageInSteamWebsite" class="icu.windea.pls.lang.actions.tools.OpenUrlActions$GameWorkshopPageInSteamWebsite" icon="icu.windea.pls.PlsIcons.General.Steam"/>
      <action id="Pls.OpenUrl.ModPageInSteam" class="icu.windea.pls.lang.actions.tools.OpenUrlActions$ModPageInSteam" icon="icu.windea.pls.PlsIcons.General.Steam"/>
      <action id="Pls.OpenUrl.ModPageInSteamWebsite" class="icu.windea.pls.lang.actions.tools.OpenUrlActions$ModPageInSteamWebsite" icon="icu.windea.pls.PlsIcons.General.Steam"/>
    </group>
    <group id="Pls.CopyPath">
      <action id="Pls.CopyPath.Steam" class="icu.windea.pls.lang.actions.tools.CopyPathActions$Steam"/>
      <action id="Pls.CopyPath.SteamGame" class="icu.windea.pls.lang.actions.tools.CopyPathActions$SteamGame"/>
      <action id="Pls.CopyPath.SteamWorkshop" class="icu.windea.pls.lang.actions.tools.CopyPathActions$SteamWorkshop"/>
      <action id="Pls.CopyPath.GameData" class="icu.windea.pls.lang.actions.tools.CopyPathActions$GameData"/>
      <action id="Pls.CopyPath.Game" class="icu.windea.pls.lang.actions.tools.CopyPathActions$Game"/>
      <action id="Pls.CopyPath.Mod" class="icu.windea.pls.lang.actions.tools.CopyPathActions$Mod"/>
    </group>
    <group id="Pls.CopyUrl">
      <action id="Pls.CopyUrl.GameStorePage" class="icu.windea.pls.lang.actions.tools.CopyUrlActions$GameStorePage" icon="icu.windea.pls.PlsIcons.General.Steam"/>
      <action id="Pls.CopyUrl.GameWorkshopPage" class="icu.windea.pls.lang.actions.tools.CopyUrlActions$GameWorkshopPage" icon="icu.windea.pls.PlsIcons.General.Steam"/>
      <action id="Pls.CopyUrl.ModPage" class="icu.windea.pls.lang.actions.tools.CopyUrlActions$ModPage" icon="icu.windea.pls.PlsIcons.General.Steam"/>
    </group>
    <group id="Pls.GotoPath">
      <separator/>
      <action id="Pls.GotoPath.Steam" class="icu.windea.pls.lang.actions.tools.GoToPathActions$Steam" icon="AllIcons.Nodes.Module"/>
      <action id="Pls.GotoPath.SteamGame" class="icu.windea.pls.lang.actions.tools.GoToPathActions$SteamGame" icon="AllIcons.Nodes.Module"/>
