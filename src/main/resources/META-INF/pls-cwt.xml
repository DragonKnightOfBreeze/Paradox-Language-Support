<idea-plugin>
  <extensions defaultExtensionNs="com.intellij">
    <fileType language="CWT" name="Cwt" fieldName="INSTANCE" implementationClass="icu.windea.pls.cwt.CwtFileType"
              extensions="cwt"/>
    <lang.parserDefinition language="CWT" implementationClass="icu.windea.pls.cwt.CwtParserDefinition"/>

    <spellchecker.support language="CWT" implementationClass="icu.windea.pls.cwt.editor.CwtSpellchecker"/>
    <lang.syntaxHighlighterFactory language="CWT" implementationClass="icu.windea.pls.cwt.editor.CwtSyntaxHighlighterFactory"/>
    <colorSettingsPage implementation="icu.windea.pls.cwt.editor.CwtColorSettingsPage"/>
    <lang.findUsagesProvider language="CWT" implementationClass="icu.windea.pls.cwt.editor.CwtFindUsagesProvider"/>
    <elementDescriptionProvider implementation="icu.windea.pls.cwt.editor.CwtFindUsagesProvider"/>
    <extendWordSelectionHandler implementation="icu.windea.pls.cwt.editor.CwtWordSelectionHandler"/>
    <annotator language="CWT" implementationClass="icu.windea.pls.cwt.editor.CwtBasicAnnotator"/>
    <lang.commenter language="CWT" implementationClass="icu.windea.pls.cwt.editor.CwtCommenter"/>
    <lang.foldingBuilder language="CWT" implementationClass="icu.windea.pls.cwt.folding.CwtFoldingBuilder"/>
    <lang.implementationTextSelectioner language="CWT" implementationClass="icu.windea.pls.cwt.codeInsight.CwtImplementationTextSelectioner"/>
    <lang.braceMatcher language="CWT" implementationClass="icu.windea.pls.cwt.codeInsight.highlighting.CwtBraceMatcher"/>
    <lang.quoteHandler language="CWT" implementationClass="icu.windea.pls.cwt.codeInsight.editorActions.CwtQuoteHandler"/>
    <statementUpDownMover implementation="icu.windea.pls.cwt.codeInsight.editorActions.CwtMover"/>
    <lang.smartEnterProcessor language="CWT" implementationClass="icu.windea.pls.cwt.codeInsight.editorActions.CwtSmartEnterProcessor"/>
    <lang.surroundDescriptor language="CWT" implementationClass="icu.windea.pls.cwt.codeInsight.surroundWith.CwtSurroundDescriptor"/>
    <lang.unwrapDescriptor language="CWT" implementationClass="icu.windea.pls.cwt.codeInsight.unwrap.CwtUnwrapDescriptor"/>
    <liveTemplateContext contextId="CWT"
                         implementation="icu.windea.pls.cwt.codeInsight.template.CwtTemplateContextType$Base"/>
    <liveTemplateContext contextId="CWT_MEMBERS" baseContextId="CWT"
                         implementation="icu.windea.pls.cwt.codeInsight.template.CwtTemplateContextType$Members"/>
    <lang.formatter language="CWT" implementationClass="icu.windea.pls.cwt.formatter.CwtFormattingModelBuilder"/>
    <codeStyleSettingsProvider implementation="icu.windea.pls.cwt.codeStyle.CwtCodeStyleSettingsProvider"/>
    <langCodeStyleSettingsProvider implementation="icu.windea.pls.cwt.codeStyle.CwtLanguageCodeStyleSettingsProvider"/>
    <navbar implementation="icu.windea.pls.cwt.navigation.CwtNavBar"/>
    <breadcrumbsInfoProvider implementation="icu.windea.pls.cwt.navigation.CwtBreadCrumbsProvider"/>
    <lang.psiStructureViewFactory language="CWT" implementationClass="icu.windea.pls.cwt.structureView.CwtStructureViewFactory"/>

    <lang.elementManipulator forClass="icu.windea.pls.cwt.psi.CwtOptionKey"
                             implementationClass="icu.windea.pls.cwt.psi.manipulators.CwtOptionKeyManipulator"/>
    <lang.elementManipulator forClass="icu.windea.pls.cwt.psi.CwtPropertyKey"
                             implementationClass="icu.windea.pls.cwt.psi.manipulators.CwtPropertyKeyManipulator"/>
    <lang.elementManipulator forClass="icu.windea.pls.cwt.psi.CwtString"
                             implementationClass="icu.windea.pls.cwt.psi.manipulators.CwtStringManipulator"/>
    <lang.elementManipulator forClass="icu.windea.pls.cwt.psi.CwtValue"
                             implementationClass="icu.windea.pls.cwt.psi.manipulators.CwtValueManipulator"/>
  </extensions>
</idea-plugin>
